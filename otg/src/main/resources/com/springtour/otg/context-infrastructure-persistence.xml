	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    
  
    
    <bean name="otg.IBatisChannelRepositoryImpl"
		class="com.springtour.otg.infrastructure.persistence.ibatis.IBatisChannelRepositoryImpl">
