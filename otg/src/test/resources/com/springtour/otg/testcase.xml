    <bean name="otg.AlipayOKRequestTestsDataPrepare"
		class="com.springtour.otg.dbop.DataPrepareOperation">
        <property name="dataReader"
			ref="otg.DataReader" />
        <property name="dataFilePath">
            <list>
                <value>/com/springtour/otg/data/ChannelDataForAlipayHappyPath.xml</value>
                <value>/com/springtour/otg/data/GatewayDataForAlipayHappyPath.xml</value>
                <value>/com/springtour/otg/data/PartnerDataForAlipayHappyPath.xml</value>
            </list>
        </property>
        <property name="dao">
            <list>
                <ref bean="otg.ChannelDataPrepareOperationDao"/>
                <ref bean="otg.GatewayDataPrepareOperationDao"/>
                <ref bean="otg.PartnerDataPrepareOperationDao"/>
            </list>
        </property>
        <property name="appendDataFilePath">
            <list>
                <value>/com/springtour/otg/data/MerchantDataForAlipayHappyPath.xml</value>
            </list>            
        </property>
        <property name="appendDao">
            <list>
                <ref bean="otg.MerchantDataPrepareOperationDao"/>
            </list>            
        </property>
    </bean>
    
