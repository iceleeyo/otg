    <sqlMap
		resource="com/springtour/otg/infrastructure/persistence/ibatis/PartnerRepositorySqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/infrastructure/persistence/ibatis/MerchantRepositorySqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/infrastructure/persistence/ibatis/TransactionRepositorySqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/infrastructure/persistence/ibatis/NotificationRepositorySqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/infrastructure/persistence/ibatis/ClockSqlMap.xml" />
    <!-- test -->
    <sqlMap
		resource="com/springtour/otg/dbop/ChannelDataPrepareOperationDaoSqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/dbop/GatewayDataPrepareOperationDaoSqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/dbop/MerchantDataPrepareOperationDaoSqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/dbop/PartnerDataPrepareOperationDaoSqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/dbop/RecommendedGatewayDataPrepareOperationDaoSqlMap.xml" />                   
    <sqlMap
		resource="com/springtour/otg/dbop/TransactionDataPrepareOperationDaoSqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/dbop/NotificationDataPrepareOperationDaoSqlMap.xml" />
    <sqlMap
		resource="com/springtour/otg/infrastructure/persistence/ibatis/MqSendFailedMessageRepositorySqlMap.xml" />
    <!-- test -->
</sqlMapConfig>